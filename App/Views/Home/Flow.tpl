		<script type="text/javascript">
			var DateIn = new Date();
			Hours = DateIn.getHours();
			Minutes = DateIn.getMinutes();
			var Time;
			if(Hours >= 10) {
				Hours = Hours + ":";
			} else {
				Hours = "0" + Hours + ":";
			}
			if(Minutes >= 10) {
				Minutes = Minutes;
			} else {
				Minutes = "0" + Minutes;
			}
			Time = Hours + Minutes;
			$(function(){
				$(".con-item .date").each(function(){
			        var txt = $(this).text();
			        var a = txt.split('-')[0];
			        var b = txt.split('-')[1];
			        if(Time >= a && Time < b){
						$(this).parents(".con-item").attr("id","time");
			        	location.href = "http://jifen.netconcepts.cn/?z=home/flow#time";
//			        	location.reload();
		        }
		    	});	
			})
			 
